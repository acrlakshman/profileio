    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "axios": "^1.1.3",
    "bootstrap": "^5.2.2",
    "concurrently": "^7.5.0",
    "jquery": "^3.6.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.20",
    "popper.js": "^1.16.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "react-highlight.js": "^1.0.7",
    "react-icons": "^4.6.0",
    "react-image-gallery": "^1.2.11",
    "react-markdown": "^8.0.3",
    "react-player": "^2.11.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "react-scripts": "^5.0.1",
    "react-scroll": "^1.8.8",
